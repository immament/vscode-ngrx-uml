{
	"name": "vscode-ngrx-uml",
	"displayName": "ngrx-uml",
	"description": "Generate ngrx/state diagram",
	"version": "0.0.1",
	"publisher": "immament",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ngrx-uml.diagram"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ngrx-uml.diagram",
				"title": "Diagram"
			}
		]
	},
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"ts-loader": "^7.0.2",
		"vscode-test": "^1.3.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"ngrx-uml": "^0.1.0",
		"typescript": "^3.8.3"
	}
}
